{
  "name": "my-website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "node node_modules/@docusaurus/core/bin/docusaurus.mjs start --no-open",
    "start:zh": "node node_modules/@docusaurus/core/bin/docusaurus.mjs start --locale zh-Hans --no-open",
    "start:en": "node node_modules/@docusaurus/core/bin/docusaurus.mjs start --locale en --no-open",
    "start:ja": "node node_modules/@docusaurus/core/bin/docusaurus.mjs start --locale ja --no-open",
    "start:es": "node node_modules/@docusaurus/core/bin/docusaurus.mjs start --locale es --no-open",
    "start:de": "node node_modules/@docusaurus/core/bin/docusaurus.mjs start --locale de --no-open",
    "build": "node --max-old-space-size=6144 node_modules/@docusaurus/core/bin/docusaurus.mjs build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "bs": "npm run clear && npm run build && npm run serve",
    "preview": "npm run build && npm run serve",
    "write-translations": "docusaurus write-translations",
    "translate": "npm run write-translations && node tool/check-doc-translations.js && node tool/translate-i18n-json.js && node tool/translate-i18n-json-itemized.js",
    "translate-i18n-json": "node tool/translate-i18n-json.js",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@docusaurus/core": "3.9.2",
    "@docusaurus/preset-classic": "3.9.2",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.9.2",
    "@docusaurus/tsconfig": "3.9.2",
    "@docusaurus/types": "3.9.2",
    "typescript": "~5.6.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=20.0"
  }
}
